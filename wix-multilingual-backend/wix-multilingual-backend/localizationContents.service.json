        "docs":
          { "summary": "Add or update localized content for a given language",
            "description":
              [ "Identify localized content by its schema and entity",
                "Language content will be merged with entity's list of language contents" ],
            "links": [],
            "examples":
              [ { "title": "basic usage",
                  "body":
                    [ "import { localizationContents } from \"wix-multilingual-backend\";",
                      "",
                      "export function createOrUpdateContentsByKey() {",
                      "  const languageTag = \"a string\";",
                      "  const languageContents = [",
                      "    {",
                      "      contentKey: {",
                      "        schemaKey: {",
                      "          appId: \"a string\",",
                      "          entityType: \"a string\",",
                      "          scope: \"GLOBAL\"",
                      "        },",
                      "        entityId: \"a string\"",
                      "      },",
                      "      languageFields: [",
                      "        {",
                      "          _id: \"a string\",",
                      "          value: {",
                      "            textValue: \"a string\"",
                      "          },",
                      "          updatedBy: \"SITE_OWNER\",",
                      "          updaterId: \"a string\",",
                      "          published: true,",
                      "          status: \"a string\",",
                      "          sequencePath: [",
                      "            {",
                      "              seqName: \"a string\",",
                      "              itemId: \"a string\"",
                      "            }",
                      "          ]",
                      "        }",
                      "      ]",
                      "    }",
                      "  ];",
                      "",
                      "  return localizationContents",
                      "    .createOrUpdateContentsByKey(languageTag, languageContents)",
                      "    .then((response) => {",
                      "      console.log(response);",
                      "    })",
                      "    .catch((error) => {",
                      "      console.error(error);",
                      "    });",
                      "}" ],
                  "extra":
                    {  } } ],
