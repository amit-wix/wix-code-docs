        "docs":
          { "summary": "Returns activity counters for a requested site member.",
            "description":
              [ "A third party can read all the public counters, set by any apps. However, third parties can only read their own private counters, not those from others' apps." ],
            "links": [],
            "examples":
              [ { "title": "basic usage",
                  "body":
                    [ "import wixActivityCountersBackend from \"wix-activity-counters-backend\";",
                      "",
                      "export function getActivityCounters() {",
                      "  const memberId = \"a string\";",
                      "",
                      "  return wixActivityCountersBackend",
                      "    .getActivityCounters(memberId)",
                      "    .then((response) => {",
                      "      console.log(response);",
                      "    })",
                      "    .catch((error) => {",
                      "      console.error(error);",
                      "    });",
                      "}" ],
